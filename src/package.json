{
  "name": "ImageUpdater",
  "description": "Updater for YGOPro Images.",
  "version": "0.0.4",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonas-vanen/image-updater.git"
  },
  "keywords": [
    "yugioh",
    "images",
    "updater"
  ],
  "author": "Jonas Vanen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonas-vanen/image-updater/issues"
  },
  "homepage": "https://github.com/jonas-vanen/image-updater#readme",
  "nexe": {
    "input": ".",
    "output": "ImageUpdater^$",
    "temp": "src",
    "browserify": {
      "requires": [],
      "excludes": [],
      "paths": []
    },
    "runtime": {
      "framework": "node",
      "version": "6.10.0",
      "js-flags": "--use_strict",
      "ignoreFlags": true
    }
  },
  "dependencies": {
    "bufferutil": "^1.3.0",
    "mkdirp": "^0.5.1",
    "request": "^2.79.0",
    "utf-8-validate": "^2.0.0"
  }
